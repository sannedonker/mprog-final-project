<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gebruiker.tafelsoefenen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.TrophyEarnedActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.CalculateActivity"
            android:windowSoftInputMode="stateVisible" />
        <activity android:name=".Activities.SpecificActivity" />
        <activity android:name=".Activities.ResetActivity" />
        <activity android:name=".Activities.InfoActivity" />
        <activity android:name=".Activities.ResultActivity" />
        <activity android:name=".Activities.ResultListActivity" />
        <activity android:name=".Activities.BeginActivity" />
        <activity android:name=".Activities.ResultListSpecificActivity" />
        <activity android:name=".Activities.TrophyActivity" />
        <activity android:name=".Activities.TrophySpecificActivity" />
    </application>

</manifest>